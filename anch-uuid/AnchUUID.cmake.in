## Anch UUID configuration +
set(ANCH_UUID_COMPILE_FLAGS "-O2 -march=native -pipe" CACHE STRING "Additional compilation flags")
#set(ANCH_UUID_COMPILE_FLAGS "-g" CACHE STRING "Additional compilation flags")

# Install path
set(ANCH_UUID_INSTALL_INC_DIR "/usr/local/include/anch" CACHE STRING "Header installation path")
set(ANCH_UUID_INSTALL_LIB_DIR "/usr/local/lib/" CACHE STRING "Libraries installation path")

# Static library build
set(ANCH_UUID_STATIC_LIB OFF CACHE BOOL "Build static library")

# Unitary tests
set(ANCH_UUID_UNITARY_TEST ON CACHE BOOL "Anch UUID unitary tests")
## Anch UUID configuration -
